{
  "name": "limerock/single-media-item",
  "title": "Single Media Item",
  "description": "This block is used to display a single image or video with an optional headline and caption. It supports WordPress alignment options",
  "category": "media",
  "icon": "format-image",
  "keywords": [],
  "acf": {
    "blockVersion": 3,
    "mode": "preview",
    "renderCallback": "LimeRockTheme_block_render_callback"
  },
  "supports": {
    "className": false,
    "align": true,
    "anchor": true,
    "color": {
      "gradient": true,
      "background": true,
      "text": false
    },
    "spacing": {
      "padding": ["top", "bottom"],
      "margin": ["top", "bottom"]
    }
  },
  "example": {
    "attributes": {
      "data": {
        "is_example": true
      }
    }
  }
}
