{% embed '@partial/block_wrapper.twig' with { extra_classes: 'block_image-and-contact-list' } %}
  {% block content %}


    {% include '@partial/media-item.twig' with { image: fields.image } only %}

    <p>{{fields.headline}}</p>

    <p>{{fields.title}}</p>

    {{fields.subtitle}}

    {% for row in fields.contacts %}


      <p>{{row['label']}}</p>

      <p>{{row['contact_name']}}</p>

      <a href="{{row['link'].url}}" alt="{{row['link'].alt}}">{{row['link'].title}}</a>

    {% endfor %}

  {% endblock %}
{% endembed %}
